{
  "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl": "./src",
    "declaration": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "lib": ["ESNext"]
  },
  "exclude": ["dist", "node_modules", "enmav.config.json", "rewriteImports.js"]
}